{
    "name": "eforce/throttle",
    "description": "(API) Rate limiting requests in CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "throttle",
        "rate",
        "limit"
    ],
    "homepage": "https://github.com/kamleshethos/throttle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamlesh",
            "email": "Kamlesh.verma@ethoswatches.com"
        }
    ],
   
    "require": {
        "cakephp/cakephp": "^3.10"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "phpunit/phpunit": "~8.5.0"
    },
    "autoload": {
        "psr-4": {
            "Eforce\\Throttle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eforce\\Throttle\\Test\\": "tests",
            "TestApp\\": "tests/test_app/src"
        }
    }
}
